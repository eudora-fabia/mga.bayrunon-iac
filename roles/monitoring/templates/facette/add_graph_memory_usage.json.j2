{
  "options": {
    "type": "area",
    "stack_mode": "normal",
    "yaxis_center": true,
    "yaxis_unit": "metric",
    "title": "Memory Usage",
    "yaxis_label": "RAM"
  },
  "template": false,
  "groups": [
    {
      "series": [
        {
          "origin": "rrd",
          "source": "{{ ansible_hostname }}",
          "metric": "memory.free",
          "name": "memory.free"
        }
      ]
    },
    {
      "series": [
        {
          "origin": "rrd",
          "source": "{{ ansible_hostname }}",
          "metric": "memory.used",
          "name": "memory.used"
        }
      ]
    },
    {
      "series": [
        {
          "origin": "rrd",
          "source": "{{ ansible_hostname }}",
          "metric": "memory.buffered",
          "name": "memory.buffered"
        }
      ]
    },
    {
      "series": [
        {
          "origin": "rrd",
          "source": "{{ ansible_hostname }}",
          "metric": "memory.cached",
          "name": "memory.cached"
        }
      ]
    },
    {
      "series": [
        {
          "origin": "rrd",
          "source": "{{ ansible_hostname }}",
          "metric": "memory.slab_recl",
          "name": "memory.slab_recl"
        }
      ]
    },
    {
      "series": [
        {
          "origin": "rrd",
          "source": "{{ ansible_hostname }}",
          "metric": "memory.slab_unrecl",
          "name": "memory.slab_unrecl"
        }
      ]
    }
  ],
  "name": "memory-usage"
}
